    {
      "name": "product",
      "item": [
        {
          "name": "get page of products",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "// Extract the ID of the first product and save as environment variable",
                  "const response = pm.response.json();",
                  "",
                  "if (response && response.content && response.content.length > 0) {",
                  "    const firstProductId = response.content[0].id;",
                  "    pm.environment.set(\"productId\", firstProductId);",
                  "    console.log(\"Saved productId:\", firstProductId);",
                  "} else if (response && Array.isArray(response) && response.length > 0) {",
                  "    const firstProductId = response[0].id;",
                  "    pm.environment.set(\"productId\", firstProductId);",
                  "    console.log(\"Saved productId:\", firstProductId);",
                  "}"
                ]
              }
            }
          ],
